    "type": "git",
    "url": "git@github.com:Doko-Demo-Doa/kage-react.git"
  },
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rimraf build release dist",
    "e2e": "ts-node --project=tsconfig.e2e.json ./e2e/app.spec.ts",
    "lint": "eslint src/** --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/** --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "electron-builder install-app-deps && node patches/fix.mjs",
    "electron:package": "cross-env NODE_ENV=production tsc -p . && electron-builder -m -w",
    "electron:build-win": "rimraf dist && tsc && vite build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ]
      }
    ]
  },
  "ts-jest": {
    "isolatedModules": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "electron",
    "react",
    "vite",
    "antd"
