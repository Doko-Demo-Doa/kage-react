{
  "name": "dora-kage",
  "version": "1.0.0",
  "author": "WabiSabi",
  "license": "Commercial",
  "homepage": "./",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:Doko-Demo-Doa/kage-react.git"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "clean": "rimraf build dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none WEBPACK_TARGET=electron npm run start\" \"wait-on http://localhost:3000 && tsc -p electron && electronmon .\"",
    "electron:build-win": "cross-env WEBPACK_TARGET=electron npm run build && tsc -p electron && electron-builder -w",
    "electron:build-mac": "npm run build && tsc -p electron && electron-builder -m",
    "electron:build-linux": "npm run build && tsc -p electron && electron-builder -l",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "build/electron/main.js",
  "dependencies": {
    "crc": "3.8.0",
    "electron-devtools-installer": "3.1.1",
    "electron-is-dev": "2.0.0",
    "electron-updater": "4.3.8",
    "sharp": "0.28.1"
  },
  "devDependencies": {
    "@ant-design/icons": "4.6.2",
    "@craco/craco": "6.1.1",
    "@storybook/addon-actions": "6.2.8",
    "@storybook/addon-essentials": "6.2.8",
    "@storybook/addon-links": "6.2.8",
    "@storybook/node-logger": "6.2.8",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.2.8",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.3",
    "@types/adm-zip": "0.4.34",
    "@types/common-tags": "1.8.0",
    "@types/crc": "3.4.0",
    "@types/dom-mediacapture-record": "1.0.7",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/fluent-ffmpeg": "2.1.17",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.39",
    "@types/pretty": "2.0.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-keyboard-event-handler": "1.5.0",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/sharp": "0.28.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "adm-zip": "0.5.5",
    "antd": "4.15.1",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.3",
    "clsx": "1.1.1",
    "common-tags": "1.8.0",
    "concurrently": "6.0.2",
    "craco-antd": "1.19.0",
    "craco-sass-resources-loader": "1.1.0",
    "cross-env": "7.0.3",
    "dayjs": "1.10.4",
    "electron": "12.0.4",
    "electron-builder": "22.10.5",
    "electronmon": "2.0.1",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.7.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
