{
  "name": "dora-kage",
  "version": "1.0.0",
  "author": "WabiSabi",
  "license": "Commercial",
  "homepage": "./",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:Doko-Demo-Doa/kage-react.git"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "clean": "rimraf build dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none WEBPACK_TARGET=electron npm run start\" \"wait-on http://localhost:3000 && tsc -p electron && electronmon .\"",
    "electron:build-win": "cross-env WEBPACK_TARGET=electron npm run build && tsc -p electron && electron-builder -w",
    "electron:build-mac": "npm run build && tsc -p electron && electron-builder -m",
    "electron:build-linux": "npm run build && tsc -p electron && electron-builder -l"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "build/electron/main.js",
  "dependencies": {
    "electron-devtools-installer": "3.1.1",
    "electron-is-dev": "2.0.0",
    "electron-updater": "4.3.8"
  },
  "devDependencies": {
    "@ant-design/icons": "4.6.2",
    "@craco/craco": "6.1.1",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.1",
    "@types/crc": "3.4.0",
    "@types/dom-mediacapture-record": "1.0.7",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/fluent-ffmpeg": "2.1.17",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/sharp": "0.28.0",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "antd": "4.15.0",
    "babel-plugin-import": "1.13.3",
    "clsx": "1.1.1",
    "concurrently": "6.0.1",
    "craco-antd": "1.19.0",
    "craco-sass-resources-loader": "1.1.0",
    "crc": "3.8.0",
    "cross-env": "7.0.3",
    "dayjs": "1.10.4",
    "electron": "12.0.2",
    "electron-builder": "22.10.5",
    "electronmon": "2.0.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "fetch-retry": "4.1.1",
    "fluent-ffmpeg": "2.1.2",
    "formik": "2.2.6",
    "history": "5.0.0",
    "html2canvas": "1.0.0-rc.7",
    "konva": "7.2.5",
    "mitt": "2.1.0",
    "moment": "2.29.1",
    "node-sass-chokidar": "1.5.0",
    "quill": "1.3.7",
    "rambdax": "7.2.0",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-device-detect": "1.17.0",
    "react-dom": "17.0.2",
    "react-konva": "17.0.2-0",
    "react-perfect-scrollbar": "1.5.8",
    "react-quill": "1.3.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "recoil": "0.2.0",
    "rimraf": "3.0.2",
    "sharp": "0.28.1",
    "spectron": "14.0.0",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4",
    "wait-on": "5.3.0",
    "web-vitals": "1.1.1",
    "yup": "0.32.9"
  },
  "electronmon": {
    "patterns": [
      "electron/*.ts",
      "!src/**"
    ]
  }
}
