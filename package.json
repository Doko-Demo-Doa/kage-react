{
  "name": "dora-kage",
  "version": "1.0.2",
  "author": "WabiSabi",
  "license": "Commercial",
  "homepage": "./",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:Doko-Demo-Doa/kage-react.git"
  },
  "scripts": {
    "start": "craco start",
    "start:player": "cross-env REACT_APP_ISPLAYER=1 craco start",
    "build": "craco build",
    "build:player": "cross-env REACT_APP_ISPLAYER=1 craco build",
    "test": "craco test --watchAll=false",
    "test:coverage": "craco test --coverage",
    "eject": "react-scripts eject",
    "clean": "rimraf build release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none WEBPACK_TARGET=electron npm run start\" \"wait-on http://localhost:3000 && tsc -p electron && electronmon .\"",
    "electron:build-win": "cross-env WEBPACK_TARGET=electron npm run build && tsc -p electron && electron-builder -w",
    "electron:build-mac": "cross-env WEBPACK_TARGET=electron npm run build && tsc -p electron && electron-builder -m",
    "electron:build-linux": "cross-env WEBPACK_TARGET=electron npm run build && tsc -p electron && electron-builder -l",
    "electron:deploy-mac": "cross-env WEBPACK_TARGET=electron npm run build && tsc -p electron && electron-builder build --mac --publish always",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "build/electron/main.js",
  "dependencies": {
    "crc": "3.8.0",
    "electron-devtools-installer": "3.2.0",
    "electron-is-dev": "2.0.0",
    "electron-updater": "4.3.9",
    "fs-extra": "10.0.0",
    "sharp": "0.28.3"
  },
