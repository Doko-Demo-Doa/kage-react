{
  "name": "dora-kage",
  "description": "Slide and quiz creation tool",
  "version": "1.0.1",
  "author": "WabiSabi",
  "license": "MIT",
  "homepage": "./",
  "main": "dist/electron-main/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:Doko-Demo-Doa/kage-react.git"
  },
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rimraf build release dist",
    "e2e": "ts-node --project=tsconfig.e2e.json ./e2e/app.spec.ts",
    "lint": "eslint src/** --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/** --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "electron-builder install-app-deps && node patches/fix.mjs",
    "electron:package": "cross-env NODE_ENV=production tsc -p . && electron-builder -m -w",
    "electron:build-win": "rimraf dist && tsc && vite build && electron-builder"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "electron",
    "react",
    "vite"
  ],
