{
  "name": "dora-kage",
  "description": "Slide and quiz creation tool",
  "version": "0.1.28",
  "author": "WabiSabi",
  "license": "Commercial",
  "homepage": "./",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:Doko-Demo-Doa/kage-react.git"
  },
  "scripts": {
    "start": "craco start",
    "start:player": "cross-env REACT_APP_ISPLAYER=1 craco start",
    "build": "cross-env CI=false craco build",
    "build:player": "cross-env CI=false cross-env REACT_APP_ISPLAYER=1 craco build",
    "test": "cross-env CI=false craco test --watchAll=false",
    "test:coverage": "cross-env CI=false craco test --watchAll=false --coverage",
    "clean": "rimraf build release build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none WEBPACK_TARGET=electron yarn run start\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build-win": "cross-env WEBPACK_TARGET=electron yarn run build && tsc -p electron && electron-builder -w",
    "electron:build-mac": "cross-env WEBPACK_TARGET=electron yarn run build && tsc -p electron && electron-builder -m",
    "electron:build-linux": "cross-env WEBPACK_TARGET=electron yarn run build && tsc -p electron && electron-builder -l",
    "electron:deploy-win": "cross-env WEBPACK_TARGET=electron yarn run build && tsc -p electron && electron-builder build --win --publish always",
    "electron:deploy-mac": "cross-env WEBPACK_TARGET=electron yarn run build && tsc -p electron && electron-builder build --mac --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "ts-jest": {
    "isolatedModules": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "build/electron/main.js",
  "dependencies": {
    "@electron/remote": "2.0.8",
    "archiver": "5.3.1",
